<body>
    <div class="container">
       
        <% result.articles.rows.forEach(article =>{ %>
            <br>
            <div class="card">
                <div class="card-header">
                    <h4><%- article.title%></h4>
                </div>
                <div class="card-body">
                    <a class="btn btn-success" href="/<%= article.slug%>">Ler Artigo</a>
                </div>
            </div>
        <% }) %>
    </div>
</body>

